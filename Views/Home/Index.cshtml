@{
    ViewData["Title"] = "Home Page";
}

<div class="text-center">
    <div class="container text-center">
        <div class="row">
            <div class="col">
                <div class="card">
                    <h1>Start Time</h1>
                    <h3>@ViewBag.startTime</h3>
                </div>
            </div>
            <div class="col">
                <div class="card">
                    <h1>End Time</h1>
                    <h3>@ViewBag.endTime</h3>
                </div>
            </div>
        </div>
        <div class="row">
            <h1>Time Remaining</h1>
            <h3>@ViewBag.remainingDay Day(S), @ViewBag.remainingHour Hour(s), @ViewBag.remainingMinute Minute(s) Remaining</h3>
        </div>
    </div>
</div>
